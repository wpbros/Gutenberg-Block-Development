	},
	"textdomain": "paragraph",
	"editorScript": "file:../../../build/js/paragraph/paragraph.js",
	"editorStyle": "file:../../../build/css/paragraph.css",
	"style": "file:../../../build/css/style-paragraph.css",
	"attributes": {
		"content": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string"
		}
	}
}
